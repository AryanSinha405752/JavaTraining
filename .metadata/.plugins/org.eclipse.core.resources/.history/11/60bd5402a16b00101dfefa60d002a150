		Sequence s=new Sequence();
		for(int i=0;i<100;i++) {
			System.out.println(s.getnext());
		}
		 
	}
	public static class worker extends Thread{
		Sequence s;
		worker( Sequence s){
			this.s=s;
		}
		public void run() {
			for(int i=0;i<100;i++) {
				System.out.println(s.getnext()+"  =  "+ Thread.currentThread().getName());
				try {
					Thread.sleep(50);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
		}
	}
