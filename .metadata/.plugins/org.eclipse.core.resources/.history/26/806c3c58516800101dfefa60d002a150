package java_basics;

public class Human {
	String name,eye;
	int age, h;
	
	public Human(){
		name="Aryan";
		eye="Brown";
		age=21;
		h=180;
	}
	
	public void speak() {
		System.out.println("Hi, my name is"+ name);
		
	}
	public void eat() {
		System.out.println(name+" has eaten");
		
	}
	public void walk() {
		System.out.println(name+ " is leaving");
		
	}
}

